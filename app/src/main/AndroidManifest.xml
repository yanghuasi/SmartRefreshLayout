            android:label="@string/app_name"></activity>
        <activity
            android:name=".adapter.activity.ItemDragActivity"
            android:exported="true"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".activity.BannerActivity"
            android:exported="true"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".activity.QQBrowserActivity"
            android:exported="true"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".activity.RefreshExampleActivity"
            android:exported="true"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".activity.RefreshPracticeActivity"
            android:exported="true"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".activity.StylesActivity"
            android:exported="true"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".multiadapter.demo.MainActivity"
            android:exported="true"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".json.FastJson"
            android:exported="true"
            android:label="@string/app_name"></activity>
    </application>

</manifest>